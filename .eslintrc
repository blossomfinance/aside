{
  "env": {
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:es-beautifier/standard"
  ],
  "globals": {},
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "script"
  },
  "plugins": [
    "es-beautifier"
  ],
  "rules": {
    "camelcase": 0,
    "curly": 2,
    "eqeqeq": 2,
    "indent": [
      2,
      2,
      {
        "SwitchCase": 1
      }
    ],
    "max-len": [
      2,
      {
        "code": 200,
        "ignoreComments": true
      }
    ],
    "max-statements-per-line": 2,
    "new-cap": 0,
    "no-eq-null": 2,
    "no-undef": 2,
    "no-unused-vars": 2,
    "no-use-before-define": [
      2,
      {
        "functions": false
      }
    ],
    "no-var": 2,
    "one-var": [
      2,
      {
        "initialized": "never",
        "uninitialized": "never"
      }
    ],
    "one-var-declaration-per-line": 2,
    "prefer-const": 2,
    "quotes": [
      2,
      "single"
    ],
    "semi": 2,
    "sort-imports": 2,
    "spaced-comment": [
      "error",
      "always",
      {
        "exceptions": [
          "-"
        ]
      }
    ],
    "strict": [
      2,
      "global"
    ]
  }
}
